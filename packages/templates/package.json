{
    "name": "@crawlee/templates",
    "version": "3.13.3",
    "description": "Templates for the crawlee projects",
    "engines": {
        "node": ">=22.0.0"
    },
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./package.json": "./package.json"
    },
    "keywords": [
        "apify",
        "headless",
        "chrome",
        "puppeteer",
        "crawler",
        "scraper"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://apify.com"
    },
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apify/crawlee"
    },
    "bugs": {
        "url": "https://github.com/apify/crawlee/issues"
    },
    "homepage": "https://crawlee.dev",
    "scripts": {
        "build": "yarn clean && yarn validate && yarn compile && yarn copy",
        "clean": "rimraf ./dist",
        "compile": "tsc -p tsconfig.build.json",
        "copy": "tsx ../../scripts/copy.ts",
        "validate": "node ./scripts/validate-manifest.mjs"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "ansi-colors": "^4.1.3",
        "inquirer": "^12.6.0",
        "tslib": "^2.8.1",
        "yargonaut": "^1.1.4",
        "yargs": "^17.7.2"
    }
}
